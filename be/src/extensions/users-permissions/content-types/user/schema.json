{
  "collectionName": "users-permissions_user",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "users-permissions": {
      "pluginName": "users-permissions"
    }
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "required": true,
      "unique": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
